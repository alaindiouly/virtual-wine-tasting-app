'>\n      <div><FontAwesomeIcon icon={faCheck} size=\"5x\" className='successfully__registred__logo' /></div>\n      <div>User successfully registered</div>\n    </div>\n    )}\n\n  </div>\n  )\n\n}\n\n","/Users/maxdraguhn/Documents/REACT-TUTORIAL/wine-form/client/src/Components/User/User.js",[],"/Users/maxdraguhn/Documents/REACT-TUTORIAL/wine-form/client/src/Components/Home/Home.js",[],{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":12,"column":6,"nodeType":"93","endLine":12,"endColumn":18,"suggestions":"94"},{"ruleId":"95","severity":1,"message":"96","line":12,"column":17,"nodeType":"97","messageId":"98","endLine":12,"endColumn":25},{"ruleId":"99","severity":1,"message":"100","line":60,"column":1,"nodeType":"101","endLine":60,"endColumn":80},{"ruleId":"95","severity":1,"message":"102","line":14,"column":10,"nodeType":"97","messageId":"98","endLine":14,"endColumn":15},{"ruleId":"91","severity":1,"message":"103","line":25,"column":6,"nodeType":"93","endLine":25,"endColumn":12,"suggestions":"104"},{"ruleId":"95","severity":1,"message":"105","line":27,"column":18,"nodeType":"97","messageId":"98","endLine":27,"endColumn":29},{"ruleId":"106","severity":1,"message":"107","line":41,"column":20,"nodeType":"108","messageId":"109","endLine":41,"endColumn":22},"no-native-reassign",["110"],"no-negated-in-lhs",["111"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'user.userId'. Either include it or remove the dependency array.","ArrayExpression",["112"],"no-unused-vars","'setValue' is assigned a value but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","'users' is assigned a value but never used.","React Hook useEffect has a missing dependency: 'checkIfUserIsInDatabase'. Either include it or remove the dependency array.",["113"],"'getAllUsers' is defined but ne